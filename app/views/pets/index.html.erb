<%= link_to 'new', newPets_path ,class: 'btn btn-info'%>
<%= link_to 'Go-Doctor', indexDoctor_path ,class: 'btn btn-info'%>
<% @pet.each do |pet|%>
<div class="card" style="width: 18rem;">
<%= image_tag pet.avatarPets %>
  <div class="card-body">
    <h5 class="card-title"><%= pet.name %></h5>
    <p class="card-text">Age:<%= pet.Age%></p>
    <p class="card-text">Weith:<%= pet.weith%></p>
    <p class="card-text">Owner:<%= pet.OwnerPets %></p>
    <p class="card-text">Phone number Owner:<%= pet.PhoneNumberOwnerPets %></p>
    <%= link_to pet.name, pet_path(pet.id), class:"btn btn-primary"%>
  </div>
</div>
<% end %>
